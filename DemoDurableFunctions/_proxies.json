{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "GetValues": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "GetValues/{key}"
      },
      "backendUri": "https://localhost/api/values/{key}"
    },
    "AllValues": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "AllValues"
      },
      "backendUri": "https://introadvazfunc.blob.core.windows.net/static-files/all-keys.json?st=2019-12-11T02%3A50%3A28Z&se=2019-12-12T02%3A50%3A28Z&sp=r&sv=2018-03-28&sr=b&sig=dwQ6CRZQGNMPkC4o%2BrmV10%2Fl5Bz9OmUoRepg%2Fsr%2BKGE%3D"
    },
    "SetValues": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "SetValues/{key}/{value}"
      },
      "responseOverrides": {
        "response.headers.Content-Type": "application/json",
        "response.body": {
          "key": "{key}",
          "value": "{value}"
        }
      }
    }
  }
}
